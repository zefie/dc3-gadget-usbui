#!/bin/bash
PWD=$(realpath "$(dirname "${0}")")
source "${PWD}/_common"
USBFILE="${1}"
shift
if [ ! -e "${USBFILE}" ]; then
	USBPATH="$(realpath "${PWD}/${USBIMG_REL_PATH}")"
	USBFILE="$(realpath "${USBPATH}/${USBFILE}")"
fi
if [ -e "${USBFILE}" ]; then
	removeAll
	waitForSettle
	loadUSBMod mass_storage "file=${USBFILE}" "removable=1" "${@}"
fi
